{
  "name": "Faceblok",
  "version": "1.0",
  "description": "Power Over Content",
  "manifest_version": 2,
  "permissions": ["storage","activeTab"],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "128": "128.png"
  },
  "web_accessible_resources": [
    "*.json"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["faceblok.js"]
    }
  ]
}
